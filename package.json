{
	"name": "@zoltu/rlp-encoder",
	"description": "Dependency free library for doing RLP encoding.",
	"version": "1.1.0",
	"repository": {
		"url": "https://github.com/Zoltu/rlp-encoder"
	},
	"license": "Unlicense",
	"main": "output-node/index.js",
	"browser": "output-es/index.js",
	"devDependencies": {
		"@types/chai": "4.1.7",
		"@types/node": "12.0.0",
		"@zoltu/bigint-helpers": "1.0.0",
		"chai": "4.2.0",
		"chai-bytes": "0.1.2",
		"recursive-fs": "2.1.0",
		"ts-node": "8.8.1",
		"typescript": "3.8.3"
	},
	"scripts": {
		"build": "tsc --project tsconfig-es.json && tsc --project tsconfig-node.json",
